<h1>Listing rounds</h1>

<table>
  <tr>
    <th>State</th>
    <th>Category</th>
    <th>Game</th>
    <th>Card state</th>
    <th>Vote state</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rounds.each do |round| %>
  <tr>
    <td><%= round.state %></td>
    <td><%= round.category %></td>
    <td><%= round.game_id %></td>
    <td><%= round.card_state %></td>
    <td><%= round.vote_state %></td>
    <td><%= link_to 'Show', round %></td>
    <td><%= link_to 'Edit', edit_round_path(round) %></td>
    <td><%= link_to 'Destroy', round, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Round', new_round_path %>
